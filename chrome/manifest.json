{
  "name": "Chrome Dosbox",
  "description": "x86 emulator with DOS on Chrome",
  "version": "0.0.0",
  "manifest_version": 2,
  "minimum_chrome_version": "31",
  "offline_enabled": true,
  "app": {
    "background": {
      "scripts": ["launch.js"],
      "transient": true
    }
  },
  "permissions": [
    {"fileSystem": ["directory", "write"]},
    "unlimitedStorage"
  ],
  "icons": {
    "16": "icons/dosbox-16.png",
    "24": "icons/dosbox-24.png",
    "32": "icons/dosbox-32.png",
    "48": "icons/dosbox-48.png",
    "256": "icons/dosbox-256.png"
  },
  "platforms": [
    {"nacl_arch": "x86-64", "sub_package_path": "_platform_specific/x86-64/"},
    {"nacl_arch": "x86-32", "sub_package_path": "_platform_specific/x86-32/"},
    {"sub_package_path": "_platform_specific/all/"}
  ]
}
